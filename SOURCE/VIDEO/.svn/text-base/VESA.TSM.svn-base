BIOS		EQU	10H
DOS		EQU	21H

;***********
;
;Puertos del chip VGA.
;
;***********

ATC_INDEX		EQU	3C0H
ATC_DATA		EQU	3C1H
TS_INDEX		EQU	3C4H
TS_DATA         EQU 3C5H
GDC_INDEX		EQU	3CEH
GDC_DATA		EQU	3CFH

		LOCALS	$$

CODIGO		SEGMENT

		ASSUME	CS:CODIGO,DS:CODIGO,ES:CODIGO,SS:NOTHING

		ORG	100H

INICIO		PROC	NEAR
		CALL	SET_VIDEO_MODE
		AND	AX,AX
		JZ	$$OK
		MOV	DX,OFFSET VESA_ERROR
		JMP	$$EXIT
$$OK:		MOV	AX,1012H
		XOR	BX,BX
		MOV	CX,256
		MOV	DX,OFFSET PALETA
		INT	BIOS
		CALL	COLORES
		MOV	SI,OFFSET LOGO
		MOV	CX,376
		MOV	DX,230
		CALL	GET_SCREEN_OFFSET
		PUSH	DI
		MOV	CX,23
		MOV	BX,181
		CALL	DRAW_BITMAP
		MOV	DX,TS_INDEX
		MOV	AX,0F02H
		OUT	DX,AX
		JMP	SHORT $$WAIT0
$$WAIT0:		MOV	DX,GDC_INDEX
		MOV	AL,5
		OUT	DX,AL
		JMP	SHORT $$WAIT1
$$WAIT1:		INC	DL
		IN	AL,DX
		AND	AL,11111100B
		OR	AL,1
		OUT	DX,AL		;Modo 1 escritura
		JMP	SHORT $$WAIT2
$$WAIT2:		POP	DI
$$MOVE_LOOP:		CMP	DI,160
		JC	$$DOS
		MOV	SI,DI
		SUB	DI,160
		MOV	CX,24
		MOV	BX,46
		PUSH	DI
		MOV	DX,WORD PTR [STATUS1]
$$WAIT:		IN	AL,DX
		TEST	AL,8
		JZ	$$WAIT
		CALL	COPY_BITMAP
		POP	DI
		JMP	SHORT $$MOVE_LOOP
$$DOS:		MOV	AX,3
		INT	BIOS
		MOV	DX,OFFSET COPYRIGHT
$$EXIT:		MOV	AH,9
		INT	DOS
		MOV	AX,4C00H
		INT	DOS
INICIO		ENDP

VESA_ERROR		DB	13,10,"Esta tarjeta VGA no es compatible VESA.",13,10,"$"

;***********
;
;Copia una zona de pantalla a otra.
;
;Entradas:	SI=origen
;	DI=destino
;	CX=altura
;	BX=pixels anchura/4
;
;***********

COPY_BITMAP		PROC	NEAR
		PUSH	DS
		PUSH	ES
		MOV	AX,0A000H
		MOV	ES,AX
		MOV	DS,AX
$$LOOPY:		PUSH	CX
		PUSH	SI
		PUSH	DI
		MOV	CX,BX
		REP	MOVSB
		POP	DI
		POP	SI
		ADD	SI,160
		ADD	DI,160
		POP	CX
		LOOP	$$LOOPY
		POP	ES
		POP	DS
		RET
COPY_BITMAP		ENDP

;***********
;
;Pone en pantalla un bitmap.
;
;Entradas:	ES:DI=direcci¢n de pantalla
;	DS:SI=apunta al bitmap
;	AL=mapa de escritura
;	CX=altura en pixels
;	BX=anchura en pixels
;
;***********

DRAW_BITMAP		PROC	NEAR
		CLD
		MOV	AH,AL
		MOV	DX,TS_INDEX
		MOV	AL,2
		OUT	DX,AL
		JMP	SHORT $$WAIT0
$$WAIT0:		INC	DL
$$LOOPY:		PUSH	CX
		PUSH	AX
		PUSH	DI
		MOV	CX,BX
$$LOOPX:		XCHG	AL,AH
		OUT	DX,AL
		JMP	SHORT $$WAIT1
$$WAIT1:		XCHG	AL,AH
		LODSB
		AND	AL,AL
		JZ	$$NEXT
		MOV	BYTE PTR ES:[DI],AL
$$NEXT:		ROL	AH,1
		ADC	DI,0
		LOOP	$$LOOPX
		POP	DI
		POP	AX
		POP	CX
		ADD	DI,160
		LOOP	$$LOOPY
		RET
DRAW_BITMAP		ENDP

;***********
;
;Pinta en la pantalla los 256 colores de la paleta por defecto.
;
;Entradas:	Ninguna.
;
;***********

COLORES		PROC	NEAR
		MOV	AX,0A000H
		MOV	ES,AX
		XOR	DI,DI
		MOV	BX,160
		MOV	DX,TS_INDEX
		MOV	AL,2
		OUT	DX,AL
		JMP	SHORT $$WAIT0
$$WAIT0:		INC	DL
		MOV	AX,1100H
		MOV	CX,256
$$LOOP0:		PUSH	CX
		MOV	CX,2
		TEST	AL,1
		JZ	$$PAR
		INC	CL
$$PAR:		PUSH	CX
		PUSH	DI
		XCHG	AL,AH
		OUT	DX,AL
		JMP	SHORT $$WAIT1
$$WAIT1:		XCHG	AL,AH
		MOV	CX,400
$$LOOP1:		MOV	BYTE PTR ES:[DI],AL
		ADD	DI,BX
		LOOP	$$LOOP1
		POP	DI
		POP	CX
		ROL	AH,1
		ADC	DI,0
		LOOP	$$PAR
		POP	CX
		INC	AL
		LOOP	$$LOOP0
		RET
COLORES		ENDP

;***********
;
;Devuelve la direcci¢n de unas determinadas coordenadas de pantalla.
;
;Entradas:	CX=coordenada Y
;	DX=coordenada X
;
;Salidas:	ES:DI=direcci¢n de pantalla
;	AL=mapa del pixel direccionado por CX y DX
;
;***********

GET_SCREEN_OFFSET	PROC
		MOV	AX,0A000H
		MOV	ES,AX
		ADD	CX,CX		;*2
		ADD	CX,CX		;*4
		ADD	CX,CX		;*8
		ADD	CX,CX		;*16
		ADD	CX,CX		;*32
		MOV	DI,CX
		ADD	CX,CX		;*64
		ADD	CX,CX		;*128
		ADD	DI,CX		;*160
		MOV	AX,DX
		SHR	DX,1		;/2
		SHR	DX,1		;/4
		ADD	DI,DX
		AND	AX,11B		;AX=plano del pixel (0-3)
		MOV	BX,OFFSET WRITE_MAPS
		XLAT
		RET
GET_SCREEN_OFFSET	ENDP

;
;Esta tabla se usa para pasar el n£mero del mapa al valor que hay que escribir
;para seleccionar ese mapa para escritura usando los registros del chip VGA.
;

WRITE_MAPS		DB	11H
		DB	22H
		DB	44H
		DB	88H

;***********
;
;Inicializa el modo de 640x400 con 256 colores.
;
;Entradas:	ninguna.
;
;Salidas:	AX=0  -> Se activo el modo correctamente.
;	AX<>0 -> No se activo el modo correctamente.
;
;***********

SET_VIDEO_MODE	PROC
		MOV	AX,4F02H
		MOV	BX,100H
		INT	BIOS		;Activa 640x400 con 256 colores VESA
		CMP	AX,4FH
		JZ	$$OK
		MOV	AX,0FFFFH
		RET
$$OK:		CLD
		CLI
		PUSH	ES
		MOV	AX,40H
		MOV	ES,AX
		MOV	BX,WORD PTR ES:[63H]
		MOV	DX,3DAH
		CMP	BL,0D4H
		JZ	$$STATUS_OK
		MOV	DL,0BAH
$$STATUS_OK:		MOV	WORD PTR [STATUS1],DX
		MOV	WORD PTR [CRTC_INDEX],BX
$$WAIT_VBL:		IN	AL,DX
		TEST	AL,8
		JZ	$$WAIT_VBL
		MOV	DX,TS_INDEX
		MOV	AL,4
		OUT	DX,AL
		INC	DX
		IN	AL,DX
		AND	AL,0F7H
		OR	AL,4
		OUT	DX,AL
		MOV	DX,GDC_INDEX
		MOV	AL,5
		OUT	DX,AL
		INC	DX
		IN	AL,DX
		AND	AL,0EFH
		OUT	DX,AL
		DEC	DX
		MOV	AL,6
		OUT	DX,AL
		INC	DX
		IN	AL,DX
		AND	AL,0FDH
		OUT	DX,AL
		MOV	DX,WORD PTR [CRTC_INDEX]
		MOV	AL,14H
		OUT	DX,AL
		INC	DX
		IN	AL,DX
		AND	AL,0BFH
		OUT	DX,AL
		DEC	DX
		MOV	AL,17H
		OUT	DX,AL
		INC	DX
		IN	AL,DX
		OR	AL,40H
		OUT	DX,AL
		MOV	AX,0A000H
		MOV	ES,AX
		XOR	DI,DI
		MOV	AX,DI
		MOV	CX,65535
		REP	STOSB
		POP	ES
		STI
		RET
SET_VIDEO_MODE	ENDP

STATUS1		DW	?
CRTC_INDEX		DW	?

