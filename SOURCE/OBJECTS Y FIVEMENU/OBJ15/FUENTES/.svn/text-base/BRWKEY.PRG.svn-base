// Dispatcher gen‚rico de teclas para un Objeto TBrowse

#include "InKey.ch"

//----------------------------------------------------------------------------//

function BrwKeyApply( oBrowse, nKey )

   do case
      case nKey == K_UP
           oBrowse:Up()

      case nKey == K_DOWN
           oBrowse:Down()

      case nKey == K_PGUP
           oBrowse:PageUp()

      case nKey == K_PGDN
           oBrowse:PageDown()

      case nKey == K_CTRL_PGUP
           oBrowse:GoTop()

      case nKey == K_CTRL_PGDN
           oBrowse:GoBottom()
   endcase

return

//----------------------------------------------------------------------------//
